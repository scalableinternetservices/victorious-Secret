<h1>Search#posts</h1>
<%= form_tag({controller: "search", action: "posts"}, method: "post") do %>
<%= label_tag(:query, "Search for:") %>
<%= text_field_tag(:query) %>
<%= submit_tag("Search") %>
<% end %>
<div>
  <table>
    <% @results.each do |result| %>
        <tr>
          <td><%= link_to result.first+" " +result.last, {:controller => :welcome, :action => :view_profile, :user_id => result.id } %></td>
        </tr>
    <% end %>
  </table>
</div>